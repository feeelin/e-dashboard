{
  "Keycloak": {
    "realm": "tulahack",
    "auth-server-url": "https://keycloak.<you-name-it>/",
    "ssl-required": "external",
    "resource": "tulahack-client",
    "verify-token-audience": true,
    "credentials": {
      "secret": "<TOP_SECRET>"
    },
    "confidential-port": 0,
    "policy-enforcer": {
      "credentials": {}
    }
  },
  "Cdn": {
    "CdnUrl": "https://cdn.<you-name-it>/"
  },
  "Web": {
    "WebAppBaseUrl": "https://cdn.<you-name-it>/"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
